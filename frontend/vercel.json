{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/v1/:path*", "destination": "https://zaiolos-gemini2api-backend.hf.space/v1/:path*" },
    { "source": "/login", "destination": "https://zaiolos-gemini2api-backend.hf.space/login" },
    { "source": "/logout", "destination": "https://zaiolos-gemini2api-backend.hf.space/logout" },
    { "source": "/admin/:path*", "destination": "https://zaiolos-gemini2api-backend.hf.space/admin/:path*" },
    { "source": "/public/:path*", "destination": "https://zaiolos-gemini2api-backend.hf.space/public/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
